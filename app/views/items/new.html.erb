<p>New Item</p>
<%= form_for @item do |f| %>
<%= f.label :home_id %>
<%= f.number_field :home_id %>
<%= f.label :category_id %>
<%= f.number_field :category_id %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :description %>
<%= f.text_field :description %>
<%= f.label :price %>
<%= f.number_field :price %>
<%= f.submit %>
<% end %>